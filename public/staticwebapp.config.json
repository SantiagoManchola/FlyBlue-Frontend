{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/*.css",
      "/*.js",
      "/*.png",
      "/*.jpg",
      "/*.jpeg",
      "/*.svg",
      "/*.gif",
      "/favicon.ico",
      "/robots.txt"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}